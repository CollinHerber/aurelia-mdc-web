    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "aurelia": "dev",
    "autoprefixer": "^10.0.1",
    "concurrently": "^5.2.0",
    "conventional-changelog-cli": "^2.0.34",
    "copyfiles": "^1.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
